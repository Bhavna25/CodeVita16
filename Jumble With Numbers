def bet(T1,T2,M):

  As=1

  i=1

  lst=[]

  if(int(T1)<=0 or int(T2)<=0 or M.isalpha()):

    return "Invalid Input"

  else:

    while(As<=int(T2)):

      As=i*(2*i-1)

      if(As>=int(T1) and As<=int(T2)):

        lst.append(As)

      i+=1

    if(len(lst)>=int(M)):

      return lst[int(M)-1]

    else:

      return "No number is present at this index"



T1,T2,M=map(str,input().split())

print(bet(T1,T2,M))
